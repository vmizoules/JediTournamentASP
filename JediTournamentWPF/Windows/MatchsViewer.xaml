<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JediTournamentWPF.Windows"
        xmlns:JediTournamentWPF="clr-namespace:JediTournamentWPF" x:Class="JediTournamentWPF.Windows.MatchsViewer"
        ResizeMode="NoResize"  
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Gestionnaire des Matchs" Height="600" Width="985" Icon="{StaticResource AppIcon}" Loaded="Window_Loaded" Closed="Window_Closed">
    <Window.Background>
        <ImageBrush ImageSource="{StaticResource MainBackground}"/>
    </Window.Background>
    <Grid x:Name="MainGrid">
        <JediTournamentWPF:CtrlMatchs x:Name="matchsController" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
