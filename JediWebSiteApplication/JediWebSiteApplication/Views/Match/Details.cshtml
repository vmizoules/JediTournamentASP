@model JediWebSiteApplication.Models.MatchModel

@{
    ViewBag.Title = "Détails du match";
}

<!-- Begin rotate box-2 -->
<div class="rotate-box-2-wrapper">
    <div class="container">
        <div class="row rotate-box-2 square-icon text-center wow zoomIn" data-wow-delay="0.6s">

            <div>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.Description)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Description)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Vainqueur)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Vainqueur)
                    </dd>

                </dl>
            </div>
            <p>
                @Html.ActionLink("Éditer", "Edit", null, null, null, "content", new { id = Model.ID }, null) |
                @Html.ActionLink("Revenir à la liste", "Index", null, null, null, "content", null, null)
            </p>
        </div>
    </div>
</div>
