@model JediWebSiteApplication.Models.TournoiModel

@{
    ViewBag.Title = "Suppression du tournoi";
    ViewBag.SubTitle = "Êtes vous sûr de vouloir le supprimer ?";
}

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Nom)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nom)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Supprimer" class="btn btn-default" /> |
            @Html.ActionLink("Revenir à la liste", "Index", null, null, null, "content", null, null)
        </div>
    }
</div>